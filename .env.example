# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# TMDB API (Required for movie search)
VITE_TMDB_API_KEY=your-tmdb-api-key

# OpenRouter API (Optional - for AI recommendations)
VITE_OPENROUTER_API_KEY=your-openrouter-api-key

# Media Server Integration (Optional)
VITE_ENABLE_MEDIA_SERVERS=true

# Emby Server Configuration (Optional)
VITE_EMBY_URL=https://your-emby-server.com
VITE_EMBY_API_KEY=your-emby-api-key

# Webhook Configuration (Optional)
# URL to send POST requests when users sign up
SIGNUP_WEBHOOK_URL=https://your-webhook-endpoint.com/signup